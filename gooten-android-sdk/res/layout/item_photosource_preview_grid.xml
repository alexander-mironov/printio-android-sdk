<?xml version="1.0" encoding="utf-8"?>
<print.io.view.GridViewAnim xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gridview_photos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:cacheColorHint="#00000000"
    android:clipToPadding="false"
    android:horizontalSpacing="8dip"
    android:layoutAnimation="@anim/layout_grid_fade"
    android:numColumns="3"
    android:listSelector="@drawable/bg_selector_global"
    android:paddingBottom="90dip"
    android:paddingLeft="10dip"
    android:paddingRight="10dip"
    android:paddingTop="4dip"
    android:scrollbars="none"
    android:stretchMode="columnWidth"
    android:verticalSpacing="8dip"
    android:visibility="visible" />
